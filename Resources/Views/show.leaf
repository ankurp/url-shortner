#extend("base")

#export("title") { URL Shortner }

#export("content") {
<div class="pure-g">
    <div class="pure-u-1-1">
        <h1 class="text-center">Your Short URL</h1>
        <p class="text-center">
            <a href="/#(shortUrl)">brl.pw/#(shortUrl)</a> ➡️ <a href="#(longUrl)">#(longUrl)</a>
        </p>
    </div>
</div>
}
