#extend("base")

#export("title") { URL Shortner }

#export("content") {
<div class="pure-g">
    <div class="pure-u-1-3"></div>
    <div class="pure-u-1-3">
        <h1>Your Short URL</h1>
        <p>
            <a href="#(shortUrl)">#(shortUrl)</a>
            for <a href="#(longUrl)">#(longUrl)</a>
        </p>
    </div>
    <div class="pure-u-1-3"></div>
</div>
}
